print("=" * 5, "buku", "=" * 5)
X = [1, 2, 3, 4, 5, 6]
print("jumlah buku awal : ", X)

X.append(7)
print("penambahan buku    : ", 7)
print("jumlah buku : ", X)

X.append(8)
print("penambahan buku    : ", 8)
print("jumlah buku : ", X)

# Mengeluarkan Data Yang Terakhir
X.pop() 
print("pengambilan buku oleh pelanggan : ", X)


print("jumlah buku : ", X)

print('\n\n')
